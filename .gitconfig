[user]
    email = jngu@yelp.com
    name = John Nguyen
[alias]
    ci = commit -v
    amend = commit --amend --no-edit
    st = status -sb
    br = branch -vv
    po = push
    hist = log --color=always --decorate --pretty=format:\"%C(auto)%h - %C(bold blue)%aE %C(green)(%cr)%C(auto)%d%C(reset) %s\" --graph --date=short
    histmore = log --color=always --decorate --pretty=format:\"%C(auto)%h - %C(bold blue)%aE %C(green)(%cr)%C(auto)%d%C(reset) %s\" --graph --date=short --stat
    behind = log --color=always --decorate --pretty=format:\"%C(auto)%h - %C(bold blue)%aE %C(green)(%cr)%C(auto)%d%C(reset) %s\" --graph --date=short @..origin/master
    ahead = log --color=always --decorate --pretty=format:\"%C(auto)%h - %C(bold blue)%aE %C(green)(%cr)%C(auto)%d%C(reset) %s\" --graph --date=short origin/master..@
    g = grep -ni
    gall = !git g
    ds = diff -C --staged --color-words
    d = diff -C --color-words
[fetch]
    prune = true
[push]
	default = simple
[branch]
	autoSetupRebase = remote
